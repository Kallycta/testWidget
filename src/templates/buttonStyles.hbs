    .lc_button-animation {
        overflow: hidden;
        width: {{ buttonSize }};
        height: {{ buttonSize }};
        {{# ifEquals buttonTemplate "leadback"}}
            background-color: {{ buttonPulseColor }}!important;
        {{/ifEquals}}
        border-radius: {{ buttonBorderRadius }}!important;
        {{# ifEquals buttonBorderAnimation "pulse" }}
            animation: pulse 2s infinite;
        {{/ifEquals}}
    }


    @-webkit-keyframes pulse {
        0% {
            -webkit-box-shadow: 0 0 0 0 {{pulseRGBStart}};
        }
        70% {
            -webkit-box-shadow: 0 0 0 30px {{pulseRGBEnd}};
        }
        100% {
            -webkit-box-shadow: 0 0 0 0 {{pulseRGBEnd}};
        }
    }


    @keyframes pulse {
        0% {
            -moz-box-shadow: 0 0 0 0 {{pulseRGBStart}};
            box-shadow: 0 0 0 0 {{pulseRGBStart}};
        }
        70% {
            -moz-box-shadow: 0 0 0 30px {{pulseRGBEnd}};
            box-shadow: 0 0 0 30px {{pulseRGBEnd}};
        }
        100% {
            -moz-box-shadow: 0 0 0 0 {{pulseRGBEnd}};
            box-shadow: 0 0 0 0 {{pulseRGBEnd}};
        }
    }

    .lc_button-animation object {
        width: 100%;
    }

    #fill-background {
        fill: {{ buttonPulseColor }}!important;
    }

    .lc_button-animation-border {
        position: absolute;
        left: 50%;
        top: 50%;
        transform: translate(-50%, -50%);
        border-radius: 50%;
        width: 90%;
        height: 90%;
        opacity: .9;
        {{# ifEquals buttonBorderAnimation "wave" }}
            animation: wave-stroke 2s infinite cubic-bezier(.42, 0, .85, .75);
            border: 2px solid {{ buttonPulseColor }};
        {{/ifEquals}}
    }

    @-webkit-keyframes wave-stroke {
        100% {
            width: 200%;
            height: 200%;
            border-color: transparent;
            opacity: 0;
        }
    }

    @keyframes wave-stroke {
        100% {
            width: 200%;
            height: 200%;
            border-color: transparent;
            opacity: 0;
        }
    }

    .widgetButton {
        position: fixed;
        {{# ifEquals mobileButtonPosition "right" }}
            right: 10px;
        {{/ifEquals}}
        {{# ifEquals mobileButtonPosition "left" }}
            left: 10px;
        {{/ifEquals}}

        bottom: 10px;
        z-index: 99;
        display: flex;
        align-items: center;
        justify-content: center;
        margin: 0;
        padding: 0;
        width: 45px;
        height: 45px;
        line-height: 16px;
        font-family: {{{baseFontFamily}}};
        border-radius: {{buttonBorderRadius}};
        outline: none;
        text-decoration: none;
        text-shadow: none;
        cursor: pointer;
        color: {{ buttonColor }};
        {{# ifNotEquals buttonTemplate "custom"}}
            background-color: {{ buttonBgColor }};
            box-shadow: 0 20px 30px 0 rgba(21, 28, 39, .15), 0 1px 2px 0 rgba(21, 28, 39, 0);
        {{/ifNotEquals}}
        transition: color 0.2s ease-in-out,
        background-color 0.2s ease-in-out,
        transform 0.2s cubic-bezier(0.35, 0.78, 1, 1),
        -webkit-transform 0.2s cubic-bezier(0.35, 0.78, 1, 1);
        {{# if buttonColorShiftAnimation}}
            -webkit-animation: widgetButtonAnimation 60s ease infinite;
            animation: widgetButtonAnimation 60s ease infinite;
        {{/if}}
    }

    .widgetButton:hover {
        {{# ifNotEquals buttonTemplate "custom"}}
            color: {{ buttonBgColor }};
            background-color: {{ buttonColor }};
        {{/ifNotEquals}}
    }

    .widgetButton:active {
        box-shadow: 0 0 0 1px rgba(128, 142, 155, 0.3);
    }

    .widgetButton:focus {
        box-shadow: 0 0 0 4px rgba(128, 142, 155, 0.3);
    }

    .widgetButton-icon {
        width: 60%;
        height: 60%;
        transform: rotate(90deg);
    }

    .lc_button_callBtn {
        transition: -webkit-transform 0.2s cubic-bezier(0.35, 0.78, 1, 1);
        transition: transform 0.2s cubic-bezier(0.35, 0.78, 1, 1), -webkit-transform 0.2s cubic-bezier(0.35, 0.78, 1, 1);
    }

    .lc_button_callBtn.big {
        -webkit-transform: scale(1.1);
        transform: scale(1.1);
    }

    @-webkit-keyframes widgetButtonAnimation {
        0% {
            -webkit-filter: hue-rotate(0deg);
            filter: hue-rotate(0deg);
        }
        100% {
            -webkit-filter: hue-rotate(360deg);
            filter: hue-rotate(360deg);
        }
    }

    @keyframes widgetButtonAnimation {
        0% {
            -webkit-filter: hue-rotate(0deg);
            filter: hue-rotate(0deg);
        }
        100% {
            -webkit-filter: hue-rotate(360deg);
            filter: hue-rotate(360deg);
        }
    }

