<style>
    .lc_widget {
        line-height: 16px;
        font-size: 16px;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
        color: #1e272e;
    }

    .lc_widget * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
    }

    .widgetModal *,
    .widgetModal *::before,
    .widgetModal *::after {
        box-sizing: border-box;
    }

    .widgetModal {
        display: grid;
        position: fixed;
        opacity: {{modalOpacity}};
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        z-index: 9999;
        padding: 0;
        overflow-y: auto;
        background-color: rgba(128, 142, 155, 0.3);
    }

    .widgetModal[aria-hidden="true"] {
        display: none;
    }

    .widgetModal-inner {
        position: relative;
        display: flex;
        flex-direction: column;
        margin: auto;
        padding: 20px 20px 40px;
        background-color: white;
        width: 100vw;
        min-height: 100vh;
        border-radius: 3px;
        box-shadow: 0px 2px 100px 0px rgba(72, 84, 96, 0.14);

        -webkit-animation: animation 1000ms linear both;
        animation: animation 1000ms linear both;
    }

    .widgetModal-aside {
        padding-left: 10px;
        display: none;
    }

    .widgetModal-close {
        position: absolute;
        top: 0;
        right: 0;
        margin: 0;
        padding: 0 0 6px;
        width: 40px;
        height: 40px;
        font-size: 34px;
        line-height: 16px;
        border: none;
        font-weight: 300;
        text-align: center;
        color: #f00;
        background-color: transparent;
        font-family: inherit;
        outline: none;
        text-decoration: none;
        box-shadow: none;
        text-shadow: none;
        cursor: pointer;
        opacity: 0.4;
        transition: opacity 0.2s cubic-bezier(0.35, 0.78, 1, 1), color 0.2s cubic-bezier(0.35, 0.78, 1, 1);
    }

    .widgetModal-close:hover {
        color: #ef5777;
    }

    .widgetModal-close:hover,
    .widgetModal-close:active,
    .widgetModal-close:focus {
        opacity: 1;
    }

    .widgetModal-logo {
        text-align: center;
    }

    .widgetModal-logo > img {
        max-width: 100%;
        max-height: 80px;
        width: {{logoWidth}};
        height: {{logoHeight}};
    }

    .widgetModal-title {
        margin-top: 20px;
        font-size: 1em;
        font-weight: 700;
        line-height: 1.25em;
        margin-bottom: 10px;
        color: #b3beca;
    }

    .widgetModal-subTitle {
        margin-bottom: 10px;
        font-size: 1.4em;
        line-height: 1.4em;
        color: #3c40c6;
    }

    .widgetModal-text {
        margin-bottom: 30px;
        line-height: 24px;
    }

    .widgetModal-form {
        display: flex;
        flex-direction: column;
    }

    .widgetModal-label {
        display: block;
        margin-bottom: 10px;
    }

    .widgetModal-labelPlaceholder {
        margin-bottom: 5px;
        display: block;
        color: #808e9b;
        font-size: 14px;
        font-weight: 300;
    }

    .widgetModal-field {
        padding: 10px;
        width: 100%;
        height: 40px;
        line-height: 24px;
        font-size: 16px;
        border: 2px solid #d2dae2;
        border-radius: 3px;
        outline: none;
        transition: border-color 0.2s cubic-bezier(0.35, 0.78, 1, 1);
    }

    .widgetModal-field:focus {
        border-color: #575fcf;
        box-shadow: 0px 2px 4px 0px rgba(72, 84, 96, 0.4);
    }

    .widgetModal-formButton {
        margin: 0 0 1px;
        padding: 10px 20px;
        width: 100%;
        min-height: 39px;
        background-color: hsl(238, 55%, 51%);
        color: #fff;
        font-size: 16px;
        font-family: inherit;
        border: none;
        border-radius: 3px;
        outline: none;
        transition: box-shadow 0.2s cubic-bezier(0.35, 0.78, 1, 1);
    }

    .widgetModal-formButton:hover {
        box-shadow: inset 0 0 0px 20px rgba(255, 255, 255, 0.1)
    }

    .widgetModal-formButton:focus-within {
        box-shadow: inset 0 0 0px 20px rgba(255, 255, 255, 0.1), 0px 2px 4px 0px rgba(72, 84, 96, 0.4);
    }

    .widgetModal-formGroup {
        margin-top: 10px;
        margin-bottom: 20px;
    }

    .widgetModal-formRadio {
        display: block;
        position: relative;
        padding: 4px 0;
    }

    .widgetModal-formRadio > input:checked,
    .widgetModal-formRadio > input:not(:checked) {
        position: absolute;
        top: 0;
        left: 0;
        width: 1px;
        height: 1px;
        opacity: 0;
        z-index: -1;
    }

    .widgetModal-formRadio > span {
        position: relative;
        display: inline-block;
        padding-left: 25px;
    }

    .widgetModal-formRadio > span::before,
    .widgetModal-formRadio > span::after {
        content: "";
        position: absolute;
    }

    .widgetModal-formRadio > span:before {
        left: 0;
        width: 18px;
        height: 18px;
        background: #fff;
        border-radius: 100%;
        border: 1px solid hsl({{secondaryColorHue}}, 18%, 75%);
    }

    .widgetModal-formRadio > input:checked + span:before,
    .widgetModal-formRadio span:hover:before,
    .widgetModal-formRadio > input:focus + span:before,
    .widgetModal-formRadio:focus > span:before {
        border-color: {{activeColor}};
    }

    .widgetModal-formRadio > input:checked + span:after {
        top: 5px;
        left: 5px;
        width: 8px;
        height: 8px;
        border-radius: 100%;
        background-color: {{activeColor}};
    }

    .widgetModal-chooseTime {
        margin-bottom: 15px;
    }

    .widgetModal-chooseTime-label {
    }

    .widgetModal-chooseTime-form {
        display: flex;
        flex-direction: column;
    }

    .widgetModal-chooseTime-item + .widgetModal-chooseTime-item {
        margin-top: 10px;
    }

    .widgetModal-chooseTime-item {
        display: flex;
        align-items: center;
    }

    .widgetModal-select {
        display: inline-flex;
        overflow: hidden;
        position: relative;

        height: 40px;
        line-height: 24px;
        font-size: 16px;
        border-radius: {{borderRadius}};
    }

    .widgetModal-select > select {
        background-color: transparent;
        border: none;
        outline: none;
        padding: 0 0 0 10px;
        line-height: 24px;
        font-size: 16px;
    }

    .widgetModal-select > select:hover ~ .widgetModal-selectDecor,
    .widgetModal-select > select:focus ~ .widgetModal-selectDecor,
    .widgetModal-select > select:active ~ .widgetModal-selectDecor {
        color: {{activeColor}};
    }

    .widgetModal-selectDecor {
        position: absolute;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        pointer-events: none;
        color: hsl({{secondaryColorHue}}, 22%, 85%);
        border: 2px solid currentColor;
        transition: border-color 0.2s cubic-bezier(0.35, 0.78, 1, 1);
    }

    .widgetModal-selectDecor:after {
        content: 'â–¼';
        position: absolute;
        top: 0;
        right: 0;
        bottom: 0;
        width: 30px;
        background-color: white;
        display: flex;
        justify-content: center;
        align-items: center;
        color: currentColor;
        border-radius: 0 {{borderRadius}} {{borderRadius}} 0;
    }

    .widgetModal-chooseTime-item.-time > span {
        display: inline-flex;
        padding: 0 4px;
        color: hsl({{secondaryColorHue}}, 18%, 75%);
    }

    .widgetModal-chooseTime-item.-date > .widgetModal-select,
    .widgetModal-chooseTime-item.-date > .widgetModal-select > select {
        width: 240px;
    }

    .widgetModal-chooseTime-item.-time > .widgetModal-select,
    .widgetModal-chooseTime-item.-time > .widgetModal-select > select {
        width: 66px;
    }

    .widgetModal-user {
        text-align: center;
    }

    .widgetModal-userAvatar {
        margin: 0 auto 20px;
        width: {{avatarMobileWidth}};
        height: auto;
        border-radius: {{avatarBorderRadius}};
        background: top center no-repeat;
        background-size: cover;
    }

    .widgetModal-userName {
        margin-bottom: 5px;
        color: #808e9b;
    }

    .widgetModal-userRole {
        text-transform: uppercase;
        color: #b3beca;
    }

    .widgetModal-poweredBy {
        position: absolute;
        right: 20px;
        bottom: 20px;
        color: #777 !important;
        background: -webkit-gradient(linear, left top, right top, color-stop(0, #777), color-stop(0.4, #777), color-stop(0.5, #fff), color-stop(0.6, #777), color-stop(1, #777));
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
        -webkit-animation: slidetounlock 40s infinite;
        text-decoration: none;
        text-align: left !important;
        font-size: 11px !important;
    }

    .widgetModal-logoPowered {
        height: 12px;
        margin-bottom: -1px;
        display: inline-block;
    }

    @-webkit-keyframes slidetounlock {
        0% {
            background-position: -720px 0;
        }
        100% {
            background-position: 720px 0;
        }
    }

    @keyframes slidetounlock {
        0% {
            background-position: -720px 0;
        }
        100% {
            background-position: 720px 0;
        }
    }

    @media (max-width: 768px) {
        .widgetModal-inner {
            margin: 0;
            height: fit-content;
        }
    }

    @media (min-width: 768px) {
        .widgetModal.is-active {
            display: flex;
            align-items: center;
            justify-content: center;
        }

        .widgetModal-inner {
          min-height: 0;
        }
        .widgetModal-inner:not(.has-logo) {
            flex-direction: row;
            padding: 40px 40px 60px;
            max-width: 680px;
        }

        .widgetModal-inner.has-logo {
            padding: 40px 60px 60px;
            max-width: 520px;
        }

        .widgetModal-aside {
            margin-right: 40px;
            display: block;
        }

        .widgetModal-title {
            margin-bottom: 14px;
            font-size: 1.4em;
            line-height: 1.05em;
            color: #b3beca;
        }

        .widgetModal-subTitle {
            font-size: 1.75em;
            line-height: 1.2em;
        }

        .widgetModal-form:not(.-columns) {
            flex-direction: row;
            align-items: flex-end;
        }

        .widgetModal-label {
            margin-right: 10px;
            margin-bottom: 0;
        }

        .widgetModal-formButton {
            width: auto;
        }

        .widgetModal-userAvatar {
            margin-top: 22px;
            margin-bottom: 10px;
            width: {{avatarWidth}};
            height: auto;
        }

        .widgetModal-userRole {
            font-size: 0.75em;
        }

        .widgetModal-chooseTime-form {
            flex-direction: row;
        }

        .widgetModal-chooseTime-item + .widgetModal-chooseTime-item {
            margin-top: 0;
            margin-left: 10px;
        }

        .intl-tel-input {
            display: block;
        }
    }

    /*in HBS font families need to put to three parenthesis because it cut quotes*/
    .widgetModal {
        font-size: {{baseFontSize}};
        font-family: {{{baseFontFamily}}};
        color: {{baseColor}};
    }

    .widgetModal-field:focus {
        border-color: {{activeColor}};
    }

    .widgetModal-title {
        font-family: {{{titleFontFamily}}};
        color: hsl({{secondaryColorHue}}, 18%, 75%);
    }

    .widgetModal-subTitle {
        color: {{activeColor}};
        font-family: {{{subTitleFontFamily}}};
    }

    .widgetModal-userName {
        color: hsl({{secondaryColorHue}}, 12%, 55%);
    }

    .widgetModal-userRole {
        color: hsl({{secondaryColorHue}}, 18%, 75%);
    }

    .widgetModal-labelPlaceholder {
        color: hsl({{secondaryColorHue}}, 18%, 45%);
    }

    .widgetModal-field {
        border-color: hsl({{secondaryColorHue}}, 22%, 85%);
    }

    .widgetModal-formButton {
        background-color: {{activeColor}};
    }

    .widgetModal-inner,
    .widgetModal-field,
    .widgetModal-formButton,
    .widgetModal-selectDecor,
    .widgetModal-select,
    .widgetModal-select > select {
        border-radius: {{borderRadius}};
    }


