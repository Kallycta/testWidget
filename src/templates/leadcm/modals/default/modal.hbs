                                </svg>
                                <span class="widgetModal-labelPlaceholder" id="lc_enterPhoneLabel">
                                    {{enterYourPhone}}
                                    <span>({{callIsFree}})</span>
                            </span>
                            </label>
                            <div class="widgetModal-formRow -phone">
                                <div class="widgetModal-formCol">
                                    <input tabindex="-1"
                                           class="widgetModal-field"
                                           id="widgetModal-field_modal"
                                           {{#unless noTypeTelInPhoneInput}}type="tel"{{/unless}}
                                           placeholder="{{phoneNumberExample}}"
                                           required>
                                </div>
                                <div class="widgetModal-formCol -auto">
                                    <button class="widgetModal-formButton" id="lc_submit_button"
                                            type="submit">{{callMe}}</button>
                                </div>
                            </div>

                            {{#if useGDPR}}
                                <label class="widgetModal-checkbox">
                                    <input type="checkbox" checked>
                                    <span>{{gdprBeforeLinkText}} <a href="{{gdprLink}}"
                                                                    target="_blank">{{gdprLinkText}}</a></span>
                                </label>
                            {{/if}}
                            {{#unless useGDPR}}
                                {{# ifNotEquals gdprLink ""}}
                                    {{# ifNotEquals gdprLink "#"}}
                                        <div class="widgetModal-gdpr">
                            <span>{{gdprBeforeLinkText}} <a href="{{gdprLink}}"
                                                            target="_blank">{{gdprLinkText}}</a></span>
                                        </div>
                                    {{/ifNotEquals}}
                                {{/ifNotEquals}}
                            {{/unless}}

                        </div>
                    </div>
                    <div class="widgetModal-selectTime">
                        {{!-- <a id="chooseTimeButton" class="widgetModal-selectTimeLink" href="#select-time">Välj vilken tid du vill bli uppringd</a> --}}
                        {{#if allowChoosingTime}}
                            <button class="widgetModal-selectTimeLink" type="button"
                                    id="chooseTimeButton">{{chooseTimeButton}}</button>
                        {{/if}}
                    </div>
                </div>
