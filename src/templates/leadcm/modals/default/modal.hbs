                {{!-- section: timer --}}
                {{!--> ./section-timer --}}
                <div class="widgetModal-section" id="widgetModal-section-timer"
                     aria-hidden="true"
                     style="display: none;"
                >

                    <div class="widgetModal-sectionInner" {{#unless useTimer}}style="display: none"{{/unless}}>
                        {{!-- section: loader --}}
                        {{!--{{> ./loader }} --}}

                        <div class="widgetModal-loader">
                            <svg class="widgetModal-loaderIcon">
                                <use xlink:href="#widgetModal-iconLoader"></use>
                            </svg>
                        </div>
                        {{!-- ./ section: loader --}}
                        <div class="widgetModal-timer">
                            <div class="widgetModal-timerLabel" id="widgetModal-timerLabel">{{callingTo}}</div>

                            <div class="widgetModal-counter">
                                <div class="widgetModal-counterItem">
                                    <div
                                            class="widgetModal-counterNumber"
                                            id="widgetModal-counterNumber-min"
                                    >00
                                    </div>
                                    <div class="widgetModal-counterLabel">min</div>
                                </div>
                                <div class="widgetModal-counterItem">
                                    <div
                                            class="widgetModal-counterNumber"
                                            id="widgetModal-counterNumber-sec"
                                    >
                                        00
                                    </div>
                                    <div class="widgetModal-counterLabel">sec</div>
                                </div>
