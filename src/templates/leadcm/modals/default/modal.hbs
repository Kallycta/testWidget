<callwidget class="widgetModal {{#if avatarUrl}}{{#if showAvatarInModal}}-portrait{{/if}}{{/if}}" aria-hidden="true">

    {{#unless disableExternalCss}}
        <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,700&display=swap" rel="stylesheet">
    {{/unless}}

    {{!-- Иконка для лоадера --}}
    <svg style="width: 0; height: 0; overflow: hidden; position: absolute; z-index: -1;"
         xmlns="http://www.w3.org/2000/svg">
        <symbol id="widgetModal-iconLoader" viewBox="0 0 38 38">
            <g fill="none" fill-rule="evenodd">
                <g transform="translate(1 1)" stroke-width="2">
                    <circle stroke-opacity=".5" cx="18" cy="18" r="18"/>
                    <path d="M36 18c0-9.94-8.06-18-18-18">
                        <animateTransform
                                attributeName="transform"
                                type="rotate"
                                from="0 18 18"
                                to="360 18 18"
                                dur="1s"
                                repeatCount="indefinite"/>
                    </path>
                </g>
            </g>
        </symbol>
    </svg>

    <div class="widgetModal-inner">
        <button class="widgetModal-close js-closeModal" type="button" title="{{closeWindow}}">&times;</button>
        {{!-- background image --}}
        <div class="widgetModal-bg"
            {{#if imageBg}}
             style="background-image: url({{imageBg}});background-repeat: no-repeat;background-size: cover;"
            {{/if}}
        >
            {{#if imageBgWaveOn}}
                <svg class="widgetModal-bgCover" viewBox="0 0 480 336" xmlns="http://www.w3.org/2000/svg">
                    <clipPath id="widgetModal-bgCover">
                        <path d="m0 0h481v261.935s-101.589-62.784-240.5 50.715c-81.169 66.32-240.5-27.308-240.5-50.715z"/>
                    </clipPath>
                </svg>
            {{/if}}
        </div>
        {{!-- ./ background image --}}

        <div class="widgetModal-container">
            {{!-- header --}}
            <div class="widgetModal-header {{#if avatarUrl}}{{#if showAvatarInModal}}has-avatar{{/if}}{{/if}}">
                {{# if logoUrl}}<img class="widgetModal-logo" src="{{logoUrl}}" alt="logo"/>{{/if}}
                <div class="widgetModal-headerInner">
                    <div class="widgetModal-headerTitles">
                        {{!--
                          Нужна возможность иметь два размера текста .widgetModal-title
                          Очень желательно учитывать переносы <br>
                        --}}
                        {{!--
                          Вариант 1. Отображается с шаблонами
                          - section-phone
                          - select-time
                          - section-select-departments
                        --}}
                        <div class="widgetModal-title">{{{title}}}</div>
                        <div class="widgetModal-subTitle">{{{subTitle}}}</div>
                        {{!--
                          Вариант 2. Отображается с шаблонами
                          - section-timer
                          - section-message
                        --}}
                        {{!-- <div class="widgetModal-title -lg">Tack för att du beställde ett samtal</div> --}}
                    </div>
                    {{#if avatarUrl}}{{#if showAvatarInModal}}
                        <div class="widgetModal-aside">
                            <div class="widgetModal-user">
                                <img src="{{avatarUrl}}" class="widgetModal-userAvatar" alt="avatar"/>
                            </div>
                            <div class="widgetModal-userName">
                                <b>{{ userName }}</b> / {{userPosition}}
                            </div>
                        </div>
                    {{/if}}{{/if}}
                </div>
            </div>
            {{!-- ./ header --}}
            <div class="widgetModal-main">
                {{!-- section: phone input --}}
                {{!--> ./section-phone --}}
                {{!--
  Для отображения процесса загрузки, необходимо добавить класс .-loading
  Вот так: .widgetModal-section.-loading
 --}}
                <div class="widgetModal-section" id="widgetModal-section-phone">
                    <div class="widgetModal-sectionInner {{#unless useGDPR}}-centered{{/unless}}">
                        {{!-- section: loader --}}
                        {{!--{{> ./loader }} --}}

                        <div class="widgetModal-loader">
                            <svg class="widgetModal-loaderIcon">
                                <use xlink:href="#widgetModal-iconLoader"></use>
                            </svg>
                        </div>
