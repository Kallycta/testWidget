                {{!-- section: message --}}
                {{!--> ./section-message --}}
                <div class="widgetModal-section" id="widgetModal-section-message" aria-hidden="true"
                     style="display: none;">
                    <div class="widgetModal-sectionInner">
                        {{!-- section: loader --}}
                        {{!--{{> ./loader }} --}}

                        <div class="widgetModal-loader">
                            <svg class="widgetModal-loaderIcon">
                                <use xlink:href="#widgetModal-iconLoader"></use>
                            </svg>
                        </div>
                        {{!-- ./ section: loader --}}
                        <div class="widgetModal-message" id="widgetModal-message">
                            Tack för din förfrågan! Vi hinner tyvärr inte kontakta dig inom utsatt tid, men hör av oss
                            så fort vi kan.
