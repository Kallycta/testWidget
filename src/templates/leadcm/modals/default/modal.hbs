                                <span class="widgetModal-labelPlaceholder">{{chooseTimeYouWillBeCalled}}</span>
                            </label>
                            <div class="widgetModal-chooseTime-form">
                                <div class="widgetModal-chooseTime-item -date">
                                    <div class="widgetModal-select">
                                        <select name="lc_fc_select_day"></select>
                                        <div class="widgetModal-selectDecor"></div>
                                    </div>
                                </div>
                                <div class="widgetModal-chooseTime-item -time">
                                    <div class="widgetModal-select">
                                        <select name="lc_fc_select_hour"></select>
                                        <div class="widgetModal-selectDecor"></div>
                                    </div>
                                    <span>:</span>
                                    <div class="widgetModal-select">
                                        <select name="lc_fc_select_minute"></select>
                                        <div class="widgetModal-selectDecor"></div>
                                    </div>
                                </div>
                            </div>
                            <div class="widgetModal-formButtonContainer">
                                <button class="widgetModal-formButton" id="widgetModal-formButton-confirmTime"
                                        type="submit">
                                    {{selectDepartmentButton}}
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
                {{!-- ./ section: select time --}}

